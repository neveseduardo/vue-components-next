{
	"name": "vue-components-next",
	"version": "0.1.0",
	"type": "module",
	"description": "Design system components for Vue 3",
	"author": "Eduardo Neves",
	"license": "MIT",
	"main": "dist/vue-components-next.umd.js",
	"module": "dist/vue-components-next.es.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/vue-components-next.es.js",
			"require": "./dist/vue-components-next.umd.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "vite",
		"prebuild": "rimraf ./dist",
		"build": "vite build --mode lib && vue-tsc -p tsconfig.build.json",
		"preview": "vite preview",
		"lint": "eslint . --ext .vue,.ts --fix",
		"typecheck": "vue-tsc --noEmit",
		"test": "vitest",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"peerDependencies": {
		"@iconify/vue": "^5.0.0",
		"vue": "^3.4.0"
	},
	"dependencies": {
		"@iconify/vue": "^5.0.0",
		"element-plus": "^2.13.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@storybook/vue3": "^10.2.1",
		"@storybook/vue3-vite": "^10.2.1",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^24.10.1",
		"@vitejs/plugin-vue": "^6.0.3",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.8.1",
		"eslint": "^9.39.2",
		"eslint-plugin-vue": "^10.7.0",
		"globals": "^17.1.0",
		"jsdom": "^27.4.0",
		"rimraf": "^6.1.2",
		"sass": "^1.97.3",
		"storybook": "^10.2.1",
		"tailwindcss": "^4.1.18",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.54.0",
		"vite": "^7.2.4",
		"vitest": "^4.0.18",
		"vue": "^3.5.27",
		"vue-tsc": "^3.2.4"
	}
}
